<h1> New Application </h1>  #vv this needs fixed
<%= form_with url: "/applicants", method: :post, local: true do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code %>
  <%= f.number_field :zip_code %>

  <%= f.label :City %>
  <%= f.check_box :adoptable, value: true %>

  <%= f.submit "Create Application"%> #should route to new app
<% end %>
